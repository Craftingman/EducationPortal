{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "ConnectionStrings": {
        "EducationPortal": "Server=localhost;Database=EducationPortal;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "ValidationPatterns": {
        "User": {
            "Email": "^.+@.+\\..+$",
            "Password": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$",
            "Name": "^([А-Я]{1}[а-яё]{1,23}|[A-Z]{1}[a-z]{1,23})$",
            "Surname": "^([А-Я]{1}[а-яё]{1,23}|[A-Z]{1}[a-z]{1,23})$"
        },
        "Material": {
            "Name": "^.{3,200}$",
            "Url": "^.{3,200}$"
        },
        "Book": {
            "Pages": "^\\d{1,5}$",
            "Year": "^\\d{4}$"
        },
        "Article": {
            "Date": "^\\d{2}/\\d{2}/\\d{4}$",
            "Source": "^.{3,200}$"
        },
        "Video": {
            "Duration": "(\\d{2}|\\d):(\\d{2}|\\d):(\\d{2}|\\d)",
            "Resolution": "\\d+x\\d+"
        },
        "Skill": {
            "Name": "^.{3,60}$"
        }
    }
}